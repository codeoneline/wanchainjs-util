{
  "name": "wanchainjs-util",
  "version": "5.2.1",
  "description": "a collection of utility functions for Wanchain",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "test:browser": "npm run build:dist && karma start karma.conf.js",
    "test:unit": "npm run build:dist && npm run build:docs && mocha",
    "test:coverage": "nyc npm run test:unit && npm run test:coveralls-upload",
    "test:coveralls-upload": "if [ $COVERALLS_REPO_TOKEN ]; then nyc report --reporter=text-lcov | coveralls; fi",
    "build:dist": "babel index.js --source-root ./ -d ./dist",
    "build:docs": "documentation build ./index.js --github --sort-order='alpha' -f md > ./docs/index.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WanJS/wanchainjs-util.git"
  },
  "keywords": [
    "ethereum",
    "utilties"
  ],
  "author": "Tyrion70 <tyrion70@gmail.com>",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/WanJS/wanchainjs-util/issues"
  },
  "homepage": "https://github.com/WanJS/wanchainjs-util",
  "dependencies": {
    "bn.js": "^4.11.0",
    "create-hash": "^1.1.2",
    "ethjs-util": "^0.1.6",
    "keccak": "^1.0.2",
    "rlp": "^2.0.0",
    "safe-buffer": "^5.1.1",
    "secp256k1": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browserify": "^14.0.0",
    "contributor": "^0.1.25",
    "coveralls": "^3.0.0",
    "documentation": "^5.2.0",
    "eslint": "^5.5.0",
    "karma": "^2.0.0",
    "karma-browserify": "^5.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-detect-browsers": "2.2.6",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^4.0.0",
    "nyc": "^13.0.1"
  }
}
